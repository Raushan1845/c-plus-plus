// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std ;
// use of struct

struct employee {
    int eid ;
    float salary ;
    char favchar;
} ;
int main() {
    struct employee ross ;
        ross.eid = 007 ;
        ross. salary = 100000000 ;
        ross.favchar = 'r';
        cout<<"ross eid is "<<ross.eid<<endl ;
        cout<<"ross salary is "<<ross.salary<<endl ;
        cout<<"ross favchar is "<<ross.favchar<<endl ;
    return 0;
}
